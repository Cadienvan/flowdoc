{
  "name": "flowdoc",
  "displayName": "FlowDoc",
  "description": "Navigate code documentation graphs from @flowdoc-* comment tags",
  "version": "0.1.4",
  "publisher": "MichaelDiPrisco",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": "https://github.com/Cadienvan/flowdoc",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/*.php",
    "workspaceContains:**/*.ts",
    "workspaceContains:**/*.js",
    "onUri"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flowdoc.pickTopic",
        "title": "FlowDoc: Pick Topic"
      },
      {
        "command": "flowdoc.openGraph",
        "title": "FlowDoc: Open Graph"
      },
      {
        "command": "flowdoc.reindex",
        "title": "FlowDoc: Reindex Workspace"
      },
      {
        "command": "flowdoc.openAtNode",
        "title": "FlowDoc: Open at Node"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "dependencies": {
    "yaml": "^2.8.2"
  }
}
